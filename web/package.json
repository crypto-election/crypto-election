    "babel-eslint": "^8.2.6",
    "babel-jest": "^25.1.0",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^2.1.1",
    "eslint": "^4.19.1",

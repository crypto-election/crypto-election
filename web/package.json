{
  "name": "crypto-election-web",
  "version": "1.0.0",
  "description": "Election with blockchain demo",
  "author": "Exonum Team <exonum@bitfury.com>",
  "repository": "https://github.com/crypto-election/crypto-election.git",
  "license": "Apache-2.0",
  "scripts": {
    "proto": "pbjs --keep-case -t static-module --root crypto_election --path node_modules/exonum-client/proto ../proto/service.proto -o ./proto/stubs.js",
    "lint": "eslint --ext .js,.vue src test",
    "compile-dev": "webpack --config webpack.dev.js",
    "compile-prod": "webpack --config webpack.prod.js",
    "test": "jest",
    "build-dev": "npm run proto && npm run lint && npm run compile-dev",
    "build-prod": "npm run proto && npm run compile-prod",
    "start": "node dist/server.js"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "dependencies": {
    "axios": "^0.19.2",
    "big-integer": "^1.6.48",
    "bootstrap": "^4.4.1",
    "exonum-client": "^0.18.3",
    "express": "^4.17.1",
    "http-proxy-middleware": "^0.19.1",
    "jquery": "^3.5.1",
    "lodash": "^4.17.15",
    "luxon": "^1.24.1",
    "moment": "^2.26.0",
    "noty": "^3.2.0-beta",
    "numeral": "^2.0.6",
    "popper.js": "^1.14.7",
    "protobufjs": "^6.8.8",
    "vue": "^2.6.10",
    "vue-datetime": "^1.0.0-beta.13",
    "vue-poll": "^0.1.8",
    "vue-router": "^3.0.6",
    "vuelidate": "^0.7.5",
    "vuex": "^3.1.1",
    "weekstart": "^1.0.1",
